- self.add_raw_data('club', self.club)

#content.club-edit-location
  -
    const navbar_base = {
      level_up: self.link_to('clubs.sole', { club_hid: self.club.hid })
    };

    const navbar_primary = {
      search_tab: 'club_topics',
      search_areas: [
        {
          title: self.t('@clubs.sole.search_club'),
          method: 'search.club_sole',
          params: { hid: String(self.club.hid) }
        }
      ]
    };

  != self.partial('@common.blocks.navbar.levelup_apps', { ...navbar_base, ...navbar_primary })

  nav.breadcrumbs-container
    .layout__container.container-fluid
      != self.partial('@users.blocks.breadcrumbs')

  .layout__container.container-fluid
    != self.partial('@common.blocks.announces')

    header.page-head
      h1.page-head__title(itemprop='name')= self.t('title')
      p.page-head__descr.d-none.d-sm-block(itemprop='description')
        = self.t('description')

    .alert.alert-warning
      = self.t('notice')

    .club-edit-location__map-container
      .club-edit-location__map

      button.club-edit-location__submit.btn.btn-primary(
        data-on-click='clubs.sole.edit.location:save'
        type='submit'
        disabled
      )= self.t('submit')
