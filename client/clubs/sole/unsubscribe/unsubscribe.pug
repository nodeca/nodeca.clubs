- var to_type = '$$ N.models.users.Subscription.types.NORMAL $$'

#content.clubs-sole-unsubscribe(data-type=to_type, data-club-hid=self.club.hid)
  != self.partial('@common.blocks.navbar.logo_apps', { search_tab: 'club_topics' })

  .layout__container.container-fluid
    != self.partial('@users.blocks.announces')

    header.page-head
      h1.page-head__title.page-loading__content(itemprop='name')= self.t('title')
      .icon.icon-loading-placeholder.page-loading__progress

    - var url = self.link_to('clubs.sole', { club_hid: self.club.hid });
    - var link = '<a href="' + pug.escape(url) + '">' + pug.escape(self.club.title) + '</a>';
    .page-loading__content!= self.t('message', { link: link })
